{
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "main",
    "next",
    "next-major",
    {
      "name": "beta",
      "prerelease": true
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "CHANGELOG.md"
      }
    ],
    "@semantic-release/npm",
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "dist/index.css",
            "label": "CSS distribution"
          },
          {
            "path": "dist/index.js",
            "label": "JS distribution"
          }
        ]
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "dist/**/*.{js,css}",
          "*.md",
          "package.json",
          "yarn-lock.json"
        ],
        "message": "chore: ðŸ¤– v${nextRelease.version} has been released [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  ],
  "preset": "angular"
}
